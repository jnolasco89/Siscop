	<queryString language="SQL">
		<![CDATA[select proy.nombrecorto,
c.fecha, c.monto, c.afavorde, c.cantidadletras, c.concepto, c.comentarios,
cu.codigo, cu.nombre, dc.saldoanterior, case dc.aplicacion when 0 then dc.monto end cargo,
case dc.aplicacion when 1 then dc.monto end abono, dc.saldoposterior,
(select nomcom from persona where id=3) solicita,
(select nomcom from persona where id=4) revisa,
(select nomcom from persona where id=5) autoriza
from cheque c
left outer join detallecheque dc on dc.idcheque=c.id
left outer join proyecto proy on proy.id=c.idproyecto
left outer join cuenta cu on cu.idproyecto= c.idproyecto and cu.codigo=dc.codigocuenta
where c.id= $P{id}]]>
	</queryString>
